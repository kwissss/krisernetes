{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  packageRules: [
    {
      description: "Override qbittorrent versioning",
      matchDatasources: ["docker"],
      matchPackageNames: ["/linuxserver//qbittorrent/"],
      versioning: "regex:^(?<compatibility>libtorrentv1)-release-(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)_v[\\d.]+-ls(?<build>\\d+)$",
    },
  ],
}